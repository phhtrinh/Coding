#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

bool isChar(char c){
    if(c >= 'A' && c <= 'Z') return 1;
    if(c >= 'a' && c <= 'z') return 1;
    return 0;
}

bool isNum(char c){
    if(c >= '0' && c <= '9') return 1;
    return 0;
}

string s;
vector<string> words;

int main(){
 //   freopen("input.txt","r",stdin);
    while(getline(cin, s)){
        if(s == "0")break;
        for(int i=0,j; i<s.length(); ++i){
     //       cout<<i<<' ';
            if(isChar(s[i])){
                for(j=i; j+1<s.length() && isChar(s[j+1]); ++j);
                words.push_back(s.substr(i,j-i+1));
                i = j;
            }else if(isNum(s[i])){
                for(j=i; j+1<s.length() && isNum(s[j+1]); ++j);
                int pos = words.size()-atoi(s.substr(i,j-i+1).c_str());
                s.replace(i,j-i+1,words[pos]);
                words.push_back(words[pos]);
                words.erase(words.begin()+pos);
                i += words.back().size() - 1;
            }
        }
   //     cout<<endl;
        cout<<s<<endl;
    }
  //  for(int i=0; i<words.size(); ++i)cout<<words[i]<<' ';cout<<endl;
}
